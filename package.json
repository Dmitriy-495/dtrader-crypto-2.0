{
  "name": "dtrader-crypto-2.0-server",
  "version": "1.0.0",
  "description": "Серверная часть торгового бота для Gate.io",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "pm2:start": "npm run build && pm2 start dist/index.js --name dtrader-crypto",
    "pm2:stop": "pm2 stop dtrader-crypto",
    "pm2:restart": "pm2 restart dtrader-crypto",
    "pm2:delete": "pm2 delete dtrader-crypto",
    "pm2:logs": "pm2 logs dtrader-crypto",
    "pm2:status": "pm2 status dtrader-crypto",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "trading",
    "crypto",
    "gate.io"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "snyk": "^1.1300.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
